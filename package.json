{
  "name": "@arrowhealth/bridge-sdk",
  "version": "2.0.1",
  "description": "Bridge SDK provides web applications the ability to integrate with the Bridge Platform.",
  "keywords": [
    "healthcare",
    "emr",
    "ehr",
    "rpm",
    "payer",
    "labs"
  ],
  "repository": "git@github.com:arrowhealth/bridge-sdk.git",
  "contributors": [
    {
      "name": "Rob Taylor",
      "email": "rob@arrowhealth.io"
    }
  ],
  "license": "SEE LICENSE",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "changelog": "auto-changelog && git add CHANGELOG.md",
    "build": "rollup -c",
    "docs:build": "npm run docs:cleanup && typedoc --out docs",
    "docs:cleanup": "./scripts/clean-docs.sh $npm_package_version",
    "docs:publish": "gh-pages -d docs",
    "release": "np --no-tests",
    "watch": "rollup -c --watch",
    "version": "npm run build && npm run changelog"
  },
  "main": "dist/index.js",
  "files": [
    "dist/*"
  ],
  "devDependencies": {
    "@rollup/plugin-html": "^0.2.4",
    "@rollup/plugin-typescript": "^8.3.1",
    "auto-changelog": "^2.4.0",
    "cacheable-request": "^7.0.2",
    "dotenv": "^14.3.2",
    "dotenv-load": "^2.0.1",
    "gh-pages": "^3.2.3",
    "np": "7.6.0",
    "rollup": "^2.70.1",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-version-injector": "^1.3.3",
    "tslib": "^2.3.1",
    "typedoc": "^0.22.13",
    "typescript": "^4.6.2"
  }
}
